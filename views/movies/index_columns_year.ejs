<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <!-- BOOTSTRAP CDN -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- PROJECT CSS    -->
    <link rel="stylesheet" href="/css/style.css">

    <!-- JQUERY CDN -->
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>

    <!-- PROJECT JS -->
    <script type="text/javascript" src="/js/app.js"></script>
  </head>
  <body>
    <div class="container">
      <nav>
        <ul class="nav nav-tabs">
          <li><a href="/movies/<%= userId %>/alphabetical_columns">Alphabetical-Columns</a></li>
          <li><a href="/movies/<%= userId %>/chronological_columns">Chronological-Columns</a></li>
          <li><a href="/movies/<%= userId %>/alphabetical">Alphabetical-List</a></li>
          <li><a href="/movies/<%= userId %>/release_date">Chronological-List</a></li>
          <li><a href="/users/<%= userId %>">NEW</a></li>
        </ul>
      </nav>

      <h1>MOVIES - Alphabetical</h1>
      <!-- parse through columns object -->
      <% Object.keys(movies).forEach(function(year) { %>
          <h2><%= year %></h2>
          <% movies[year].forEach(function(movie) { %>
            <p><a href="/movies/<%= userId %>/<%= movie.id %>"><%= movie.Title %></a></p>
          <% }) %>
      <% }); %>
    </div>
  </body>
</html>
